  },
  "keywords": [
    "file",
    "static",
    "server",
    "express"
  ],
  "author": "nashaofu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashaofu/estatic.git"
  },
  "bugs": {
    "url": "https://github.com/nashaofu/estatic/issues"
  },
  "homepage": "https://github.com/nashaofu/estatic#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "detect-port": "^1.3.0",
    "express": "^4.17.1",
    "open": "^7.0.4",
    "qrcode": "^1.4.4",
    "serve-index": "^1.9.1",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/detect-port": "^1.3.0",
