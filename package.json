{
  "name": "estatic",
  "version": "0.1.0",
  "description": "A simple static server",
  "bin": {
    "estatic": "estatic.js"
  },
  "files": [
    "lib",
    "estatic.js"
  ],
  "scripts": {
    "dev": "tsc --watch",
